#
#  Primary Targets.
#

#  The default target.

default: $(GAMEFILE)

all: install.tag

util: utility.tag

install: install.tag

utility.tag: $(INCL)\date.h $(INCL)\trap.h    $(INCL)\onames.h	       \
	$(INCL)\pm.h	  monstr.c   vis_tab.c	$(INCL)\filename.h\
	lev_comp.exe	 dgn_comp.exe	 recover.exe $(TILEUTIL)
	$(ECHO) utilities made > utility.tag

install.tag: dat.tag $(GAMEFILE)
ifeq ($(USE_DLB),Y)
	$(COPY) $(DAT)\nhshare       $(GAMEDIR)
	$(COPY) $(DAT)\nhushare      $(GAMEDIR)
	$(COPY) $(DAT)\license       $(GAMEDIR)
else
	$(COPY) $(DAT)\*.            $(GAMEDIR)
	$(COPY) $(DAT)\*.dat         $(GAMEDIR)
	$(COPY) $(DAT)\*.lev         $(GAMEDIR)
#	$(DEL) $(GAMEDIR)\makefile.
endif
ifneq ($(USE_GTK),N)
	$(COPY) $(WGTK)\GTKRC.w32    $(GAMEDIR)\gtkrc
	$(COPY) rip.xpm              $(GAMEDIR)
	$(COPY) credit.xpm           $(GAMEDIR)
	$(COPY) x11tiles             $(GAMEDIR)
	$(COPY) x11bigtiles          $(GAMEDIR)
	$(COPY) x11big3dtiles        $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\glib\glib*.dll $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\glib\gmodule\gmodule*.dll $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\gtk+\gdk\gdk*.dll $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\gtk+\gtk\gtk*.dll $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\gtk+\gtk\gtk*.dll $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\libiconv-1.3\src\iconv*.dll $(GAMEDIR)
#	$(COPY) $(MW32GTK)\src\intl\gnu*.dll $(GAMEDIR)
	$(COPY) $(GNUDLL)\\*.dll $(GAMEDIR)
ifeq ($(GTK_TARGET),2.0)
	$(RCOPY) $(MW32GTK)\etc $(GAMEDIR)
	-$(RDEL) $(GAMEDIR)\lib
	mkdir $(GAMEDIR)\lib
	$(RCOPY) $(MW32GTK)\lib\pango $(GAMEDIR)\lib
	$(RCOPY) $(MW32GTK)\lib\glib-2.0 $(GAMEDIR)\lib
	$(RCOPY) $(MW32GTK)\lib\gtk-2.0 $(GAMEDIR)\lib
