ifeq ($(SUPPRESS_GRAPHICS),Y)
	TEXTIO       =
	TILOBJ       =
	PLANAR_TIB   =
	OVERVIEW_TIB =
	TILEUTIL     = 
	TILEFILES    = 
	TILEFILES2   = 
	IGIFREADERS  =
