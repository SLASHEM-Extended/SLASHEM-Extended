	 echo $(VOBJ08) >> $(subst /,\,$@)
	 echo $(VOBJ09) >> $(subst /,\,$@)
	 echo $(VOBJ10) >> $(subst /,\,$@)
	 echo $(VOBJ11) >> $(subst /,\,$@)
	 echo $(VOBJ12) >> $(subst /,\,$@)
	 echo $(VOBJ13) >> $(subst /,\,$@)
	 echo $(VOBJ14) >> $(subst /,\,$@)
	 echo $(VOBJ15) >> $(subst /,\,$@)
	 echo $(VOBJ16) >> $(subst /,\,$@)
