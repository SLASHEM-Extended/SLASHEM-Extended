	TEXTIO2     = $(O)tiletex2.o $(O)tiletxt2.o $(O)drawing.o $(O)decl.o $(O)monst.o \
			$(O)objects.o $(O)stubvid.o

	ifeq ($(USE_ALLEGRO),Y)
