$(DAT)/dlb.lst:	$(U)dlb_main.exe $(DAT)/data $(DAT)/rumors $(DAT)/dungeon \
	    $(DAT)/oracles $(DAT)/quest.dat $(O)sp_lev.tag
	@$(subst /,\,echo dat done >$(O)dat.tag)
